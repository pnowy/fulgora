    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#9E7BB0"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword,keyword.operator.new,keyword.operator.delete,keyword.operator.static,keyword.operator.this,keyword.operator.expression",
      "settings": {
        "foreground": "#FFA500"
